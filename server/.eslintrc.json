{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "indent": [
            "error",
            "tab",
            { "SwitchCase": 1 }
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-tabs": "off"
    }
}